{
	"express": {
		"port": "<%= props.nodePort %>",
		"middleware": [
			"bodyParser",
			"cors"
		]
	},
	"cluster": {
		"maxWorkers": 1
	},
	"bodyParser": {
		"json": {}
	},

	"cors": {
		"credentials": true,
		"origin": [
			"http://localhost:8080",
			"http://localhost:3000",
			"http://localhost:3001",
			"http://localhost:3002"
		]
	}
}
